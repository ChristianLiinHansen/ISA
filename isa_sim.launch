<!-- ISA launch file -->

<launch>
 	<!-- Running the vision_node -->
	<node 
		pkg = "isa_project" type="vision_node" name ="vision_node" output="screen">
 	</node>

	<!-- Running the decision_node -->
	 <node 
		pkg = "isa_project" type="decision_node" name ="decision_node" output="screen">
 	</node>

	<!-- Running the decision_node -->
	 <node 
		pkg = "isa_project" type="gyro_node" name ="gyro_node" output="screen">
 	</node>

<!--
	<group ns="transforms">
		<node pkg="tf" type="static_transform_publisher" name="base_link_broadcaster" args="0 0 0 0 0 0 base_footprint base_link 100"/>
		<node pkg="tf" type="static_transform_publisher" name="imu_link_broadcaster" args="0.0 0 0.0 0 0 0 base_footprint imu_link 100"/>
		<node pkg="tf" type="static_transform_publisher" name="odom_broadcaster" args="0.0 0 0.0 0 0 0 odom_combined imu_link 100"/>
	</group>
-->
	<group ns="fmSensors">
		<node pkg="vectornav_vn100" name="imu_node"  type="vectornav_vn100_node" output="screen" >
			<param name="vectornav_vn100_sub" value="/fmData/imu_rx"/>
			<param name="imu_pub" value="/fmInformation/imu"/>
			<param name="frame_id" value="imu_link"/>
			<param name="use_enu" value="true"/>
			<param name="covariance_x" value="0.001"/>
			<param name="covariance_y" value="0.001"/>
			<param name="covariance_z" value="0.001"/>
		 </node>
	</group>

	<group ns="fmLib">
		<node pkg="serial_string" name="imu_serial_string_node" type="serial_string_node" output="screen" >
			<param name="publisher_topic" value="/fmData/imu_rx" />	 
			<param name="subscriber_topic" value="/fmData/imu_tx" />
			<param name="device" value="/dev/ttyUSB0" />	
			<param name="baudrate" value="115200" />
			<param name="termination_character" value="10"/>
		</node> 	
	</group>

	<!-- Running the simulation frobit_v2_demo -->
	
	<node 
		pkg = "frobit_v2_demo" type="frobit_sim_wpt_nav" name ="frobit_sim_wpt_nav" output="screen">
 	</node>

	<!-- Converting the twist_stampled to twist to run the simulation frobit_v2_demo -->
	<group ns="fmLib">
		<node pkg="twist_stamped_to_twist" name="converter" type="cmd_vel_converter.py">	
			<param name="cmd_vel_sub" value="/fmCommand/cmd_vel" />
			<param name="cmd_vel_pub" value="/cmd_vel" />
			<param name="deadman_sub" value="/fmCommand/deadman" />
		</node>
	</group>

	<!--	 
	<node 
		pkg = "isa_project" type="listener_node" name ="listener_node" output="screen">
 	</node>
	-->
	
</launch>



